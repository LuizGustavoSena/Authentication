{
  "compilerOptions": {
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "target": "es2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "vitest/globals"
    ]
  }
}