    "knex:latest:prd": "node --env-file=.env.prd ./node_modules/knex/bin/cli.js migrate:latest",
    "start": "node dist/server.js",
    "test": "vitest"
  },
  "dependencies": {
    "@fastify/cors": "8.5.0",
    "@types/express": "5.0.5",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "16.3.1",
    "express": "5.1.0",
    "express-rate-limit": "8.2.1",
    "fastify": "4.24.3",
    "helmet": "8.1.0",
    "jwt-simple": "0.5.6",
    "knex": "3.1.0",
    "pg": "8.18.0",
    "sqlite3": "5.1.7",
    "toobusy-js": "0.5.1",
    "uuid": "11.1.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.2.0",
    "@types/cors": "2.8.19",
    "@types/jwt-simple": "0.5.36",
    "@types/node": "20.8.9",
    "@types/toobusy-js": "0.5.4",
    "eslint": "8.52.0",
    "ts-node": "10.9.1",
    "tsx": "3.14.0",
